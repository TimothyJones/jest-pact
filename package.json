{
  "name": "jest-pact",
  "version": "0.9.0-beta.v3",
  "description": "a pact adaptor for jest",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project . --fix",
    "build": "rimraf dist && tsc",
    "test": "jest --coverage --runInBand --verbose",
    "test:ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "dist:ci": "npm run lint && npm run test:ci && npm run build && mv dist/index.* .",
    "dist:local:": "npm run lint && npm run test && npm run build && mv dist/index.* .",
    "deploy:prepare": "./scripts/create_npmrc_file.sh",
    "release": "standard-version",
    "release:dryrun": "npm run test && npm publish --dry-run",
    "release:publish": "npm run test && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pact-foundation/jest-pact.git"
  },
  "author": "YOU54F",
  "contributors": [
    {
      "name": "Yousaf Nabi",
      "url": "https://github.com/YOU54F"
    },
    {
      "name": "Timothy Jones",
      "url": "https://github.com/TimothyJones"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pact-foundation/jest-pact/issues"
  },
  "homepage": "https://github.com/pact-foundation/jest-pact/#readme",
  "devDependencies": {
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "@pact-foundation/pact": "v10.0.0-beta.16",
    "@types/supertest": "2.0.8",
    "coveralls": "3.0.9",
    "cross-env": "7.0.2",
    "husky": "4.2.3",
    "jest": "26.4.0",
    "jest-junit": "10.0.0",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "standard-version": "^9.0.0",
    "supertest": "4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@pact-foundation/pact": "^v10.0.0-beta.16",
    "jest": "^24.0.0 || ^25.0.0 || ^26.0.0"
  },
  "dependencies": {
    "@types/jest": "26.0.10"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "tslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "rules": {
      "header-max-length": [
        0,
        "always",
        72
      ],
      "subject-case": [
        2,
        "always",
        "sentence-case"
      ]
    }
  }
}
