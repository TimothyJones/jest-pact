{
  "name": "jest-pact",
  "version": "0.1.0",
  "description": "a pact adaptor for jest",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest --coverage",
    "release": "npm run test && npm run build && mv dist/index.* . && npm publish",
    "release:dryrun": "npm run test && npm run build && mv dist/index.* . && npm publish --dry-run",
    "lint": "tslint --project . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOU54F/jest-pact.git"
  },
  "author": "YOU54F",
  "contributors": [
    {
      "name": "Yousaf Nabi",
      "url": "https://github.com/YOU54F"
    },
    {
      "name": "Timothy Jones",
      "url": "https://github.com/TimothyJones"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YOU54F/jest-pact/issues"
  },
  "homepage": "https://github.com/YOU54F/jest-pact/#readme",
  "devDependencies": {
    "@pact-foundation/pact": "8.2.4",
    "@types/supertest": "2.0.7",
    "cross-env": "5.2.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "rimraf": "2.6.3",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-no-focused-test": "0.5.0",
    "typescript": "3.5.1"
  },
  "peerDependencies": {
    "jest": "~24.8.0",
    "@pact-foundation/pact": "~8.2.4"
  },
  "dependencies": {
    "@types/jest": "24.0.13"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
